{
    "tabnine.experimentalAutoImports": true,
    "workbench.iconTheme": "material-icon-theme",
    "security.workspace.trust.untrustedFiles": "open",
    "editor.linkedEditing": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "kite.showWelcomeNotificationOnStartup": false,
    "workbench.editorAssociations": {
        "*.class": "default"
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "vscode-edge-devtools.webhint": false,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features",
        "editor.suggest.insertMode": "replace",
        "editor.tabSize": 2
    },
    "workbench.colorTheme": "CRT UI - Default with alt syntax color",
    "bracket-pair-colorizer-2.depreciation-notice": false,
    "liveServer.settings.useWebExt": true,
    "phpcs.trace.server": "messages",
    "files.autoSave": "onWindowChange",
    "editor.wordWrap": "on",
    "boot-java.change-detection.on": true,
    "boot-java.scan-java-test-sources.on": true,
    "boot-java.rewrite.globa-commands.on": true,
    "boot-java.support-spring-xml-config.on": true,
    "boot-java.validation.spel.on": true,
    "spring-boot.ls.problem.application-properties.PROP_DUPLICATE_KEY": "IGNORE",
    "spring-boot.ls.problem.application-properties.PROP_EXPECTED_DOT_OR_LBRACK": "IGNORE",
    "spring-boot.ls.problem.application-properties.PROP_INVALID_BEAN_NAVIGATION": "IGNORE",
    "spring-boot.ls.problem.application-properties.PROP_INVALID_BEAN_PROPERTY": "IGNORE",
    "spring-boot.ls.problem.application-properties.PROP_INVALID_INDEXED_NAVIGATION": "IGNORE",
    "spring-boot.ls.problem.application-properties.PROP_NO_MATCHING_RBRACK": "IGNORE",
    "spring-boot.ls.problem.application-properties.PROP_NON_INTEGER_IN_BRACKETS": "IGNORE",
    "spring-boot.ls.problem.application-properties.PROP_SYNTAX_ERROR": "IGNORE",
    "spring-boot.ls.problem.application-properties.PROP_VALUE_TYPE_MISMATCH": "IGNORE",
    "spring-boot.ls.problem.application-yaml.YAML_DEPRECATED_ERROR": "IGNORE",
    "AllAutocomplete.disableNumberHandling": true,
    "AllAutocomplete.dontContributeToSelf": true,
    "AllAutocomplete.cycleOpenDocumentsOnLaunch": true,
    "liveServer.settings.CustomBrowser": "microsoft-edge",
    "liveServer.settings.donotVerifyTags": true,
    "liveServer.settings.fullReload": true,
    "yaml.trace.server": "verbose",
    "codestream.serverUrl": "https://api.codestream.com",
    "codestream.email": "repdesystem@gmail.com",
    "redhat.telemetry.enabled": true,
    "window.zoomLevel": -1,
    "editor.renderControlCharacters": false,
    "editor.columnSelection": true
}